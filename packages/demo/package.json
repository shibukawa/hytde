{
  "name": "@hytde/demo",
  "version": "0.1.2",
  "homepage": "https://github.com/shibukawa/hytde",
  "author": "Yoshiki Shibukawa",
  "license": "Apache-2.0",
  "private": true,
  "type": "module",
  "scripts": {
    "predev": "node ../../scripts/copy-msw-worker.mjs",
    "dev": "vite",
    "dev:spa": "HYTDE_DEMO_SPA=true HYTDE_DEMO_PATH_MODE=hash vite",
    "dev:spa:hash": "HYTDE_DEMO_SPA=true HYTDE_DEMO_PATH_MODE=hash vite",
    "dev:spa:path": "HYTDE_DEMO_SPA=true HYTDE_DEMO_PATH_MODE=path vite",
    "dev:demo:prod:hash": "HYTDE_DEMO_DEBUG=false HYTDE_DEMO_PATH_MODE=hash vite",
    "dev:demo:prod:path": "HYTDE_DEMO_DEBUG=false HYTDE_DEMO_PATH_MODE=path vite",
    "dev:demo:debug:hash": "node ../../scripts/copy-msw-worker.mjs && HYTDE_DEMO_DEBUG=true HYTDE_DEMO_PATH_MODE=hash vite",
    "dev:demo:debug:path": "node ../../scripts/copy-msw-worker.mjs && HYTDE_DEMO_DEBUG=true HYTDE_DEMO_PATH_MODE=path vite",
    "prebuild": "node ../../scripts/copy-msw-worker.mjs",
    "build": "vite build",
    "preview": "vite preview",
    "preview:demo:prod": "HYTDE_DEMO_DEBUG=false HYTDE_DEMO_PATH_MODE=hash HYTDE_DEMO_OUT_DIR=dist/prod vite build && vite preview --outDir dist/prod",
    "preview:demo:debug": "node ../../scripts/copy-msw-worker.mjs && HYTDE_DEMO_DEBUG=true HYTDE_DEMO_PATH_MODE=hash HYTDE_DEMO_OUT_DIR=dist/debug vite build && vite preview --outDir dist/debug",
    "ssr:dev": "npm --prefix ../.. run build && vite build && tsc -p tsconfig.ssr.json && node dist/ssr/ssr-server.mjs",
    "test": "echo \"no tests\""
  },
  "dependencies": {
    "@hytde/parser": "0.1.2",
    "@hytde/runtime": "0.1.2"
  },
  "devDependencies": {
    "@hytde/standalone": "0.1.2",
    "@hytde/ssr": "0.1.2",
    "@hytde/vite-plugin": "0.1.2",
    "@tailwindcss/vite": "^4.0.0",
    "@hono/node-server": "^1.19.9",
    "hono": "^4.6.8",
    "msw": "^2.3.1",
    "tailwindcss": "^4.0.0"
  }
}
