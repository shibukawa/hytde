{
  "name": "@hytde/precompile",
  "version": "0.1.2",
  "homepage": "https://github.com/shibukawa/hytde",
  "author": "Yoshiki Shibukawa",
  "license": "Apache-2.0",
  "private": false,
  "publishConfig": {
    "access": "public"
  },
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/prod/index.js"
    },
    "./debug": {
      "types": "./dist/index.d.ts",
      "default": "./dist/debug/index.js"
    },
    "./no-auto": {
      "types": "./dist/index.d.ts",
      "default": "./dist/prod-manual/index.js"
    },
    "./no-auto-debug": {
      "types": "./dist/index.d.ts",
      "default": "./dist/debug-manual/index.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsc -b && node ../../scripts/build-precompile.mjs",
    "test": "echo \"no tests\""
  },
  "dependencies": {
    "@hytde/runtime": "0.1.2",
    "@hytde/parser": "0.1.2",
    "@hytde/standalone": "0.1.2"
  },
  "devDependencies": {
    "tailwindcss": "^4.0.0"
  }
}
