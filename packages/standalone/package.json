{
  "name": "@hytde/standalone",
  "version": "0.1.1",
  "homepage": "https://github.com/shibukawa/hytde",
  "author": "Yoshiki Shibukawa",
  "license": "Apache-2.0",
  "private": false,
  "publishConfig": {
    "access": "public"
  },
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/prod/index.js"
    },
    "./debug": {
      "types": "./dist/index.d.ts",
      "default": "./dist/debug/index.js"
    },
    "./debug-api": {
      "types": "./dist/index.d.ts",
      "default": "./dist/debug-manual/index.js"
    },
    "./msw-debug": {
      "types": "./dist/msw-debug.d.ts",
      "default": "./dist/msw-debug.js"
    },
    "./no-auto": {
      "types": "./dist/index.d.ts",
      "default": "./dist/prod-manual/index.js"
    },
    "./no-auto-debug": {
      "types": "./dist/index.d.ts",
      "default": "./dist/debug-manual/index.js"
    },
    "./mockServiceWorker.js": "./dist/mockServiceWorker.js"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsc -b && node ../../scripts/build-standalone.mjs",
    "test": "echo \"no tests\""
  },
  "dependencies": {
    "@hytde/runtime": "0.1.1",
    "@hytde/parser": "0.1.1",
    "msw": "^2.3.1"
  }
}
